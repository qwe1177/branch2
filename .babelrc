{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions", "ie >= 9"]
        },
        "modules": false,
        "loose": true
      }
    ],
    "stage-0",
    "react"
  ],
  "plugins": [
      "babel-plugin-transform-decorators-legacy",
      "react-hot-loader/babel",
      [
        "import",
        {
          "libraryName": "antd",
			"style": "css"
        }
      ]
    ]
}
